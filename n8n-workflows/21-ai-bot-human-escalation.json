            }
          ]
        },
        "sendBody": true,
        "bodyContentType": "json",
        "specifyBody": "json",
        "jsonBody": "={{ {\n  eventId: $json.eventId,\n  reason: $json.reason,\n  priority: $json.priority,\n  escalatedAt: new Date().toISOString()\n} }}"
      },
