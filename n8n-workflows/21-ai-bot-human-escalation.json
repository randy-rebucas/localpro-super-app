{
  "name": "LocalPro - AI Bot Human Escalation",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "human-escalation",
        "responseMode": "responseNode",
        "options": {}
      },
      "id": "webhook-trigger",
      "name": "Human Escalation Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 300],
      "webhookId": "human-escalation-webhook"
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ { \"status\": \"received\", \"message\": \"Escalation received and being processed.\" } }}"
      },
      "id": "webhook-response",
      "name": "Webhook Response",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [450, 300]
    },
    {
      "parameters": {
        "jsCode": "// Extract escalation data\nconst webhook = $input.first().json;\n\nreturn {\n  json: {\n    eventId: webhook.eventId,\n    reason: webhook.reason || webhook.escalationReason,\n    priority: webhook.priority || 'medium', // low, medium, high, critical\n    eventData: webhook.eventData || {},\n    context: webhook.context || {},\n    interactionId: webhook.interactionId,\n    timestamp: webhook.timestamp || new Date().toISOString()\n  }\n};"
      },
      "id": "extract-escalation-data",
      "name": "Extract Escalation Data",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [650, 300]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "check-priority",
              "leftValue": "={{ $json.priority }}",
              "rightValue": "critical",
              "operator": {
                "type": "string",
                "operation": "equals"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "check-priority",
      "name": "Check Priority",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [850, 300]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "={{ $env.LOCALPRO_API_URL }}/api/notifications",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "X-API-Key",
              "value": "={{ $env.LOCALPRO_API_KEY }}"
            },
            {
              "name": "X-API-Secret",
              "value": "={{ $env.LOCALPRO_API_SECRET }}"
            },
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        },
        "sendBody": true,
        "bodyContentType": "json",
        "specifyBody": "json",
        "jsonBody": "={{ {\n  userId: $env.ADMIN_USER_ID,\n  type: 'escalation',\n  title: 'AI Bot Escalation - ' + $json.priority.toUpperCase(),\n  message: 'Event ID: ' + $json.eventId + '\\nReason: ' + $json.reason + '\\nPriority: ' + $json.priority,\n  data: {\n    eventId: $json.eventId,\n    reason: $json.reason,\n    priority: $json.priority,\n    eventData: $json.eventData\n  },\n  priority: $json.priority\n} }}"
      },
      "id": "notify-admin-critical",
