          ]
        },
        "sendBody": true,
        "bodyContentType": "json",
        "specifyBody": "json",
        "jsonBody": "={{ {\n  to: $env.ADMIN_EMAIL,\n  subject: 'AI Bot Escalation - ' + $json.priority.toUpperCase() + ' Priority',\n  template: 'escalation-alert',\n  data: {\n    eventId: $json.eventId,\n    reason: $json.reason,\n    priority: $json.priority,\n    eventData: JSON.stringify($json.eventData, null, 2),\n    timestamp: $json.timestamp\n  }\n} }}"
      },
      "id": "send-email-alert",
